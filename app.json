    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.upmost.dailish"
    },
    "android": {
      "package":"com.upmost.dailish",
      "versionCode": 2
    }
  }
}
