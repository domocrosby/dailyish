{
  "expo": {
    "name": "dailyish",
    "slug": "dailyish",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./logo.png",
